public class Day1 {
	public static void main(String[] args) {
		int x=20;
		final int y=30;
		String months="feb";//dec, mar,
				switch(months) {
				default:
					System.out.println("others");
		case "jan":
			System.out.println("jan");
		case "feb":
			System.out.println("Feb");
		case "mar":
			System.out.println("mar");
				}
	}
}